<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Chalu Squirrels</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,900,700,300,300italic|Lato:300,400,700,900|Poppins:300,400,500,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Dancing+Script" rel="stylesheet">
    <link href="~/Content/owl.carousel.css" rel="stylesheet" />
</head>
<body>
    @Html.Partial("Header")

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer class="text-center">            
            <p>&copy; @DateTime.Now.Year - Chalu Squirrels</p>
        </footer>
    </div>

    <a href="#top" class="go-to-top" title="Go to Top">
        <b class="glyphicon glyphicon-chevron-up"></b>
    </a>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/owl.carousel.js"></script>
    @RenderSection("scripts", false)
</body>
</html>